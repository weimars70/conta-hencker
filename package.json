{
  "name": "vue-quasar-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "backend": "node backend/src/main.js",
    "realtime": "node backend/src/realtime-server.mjs",
    "start:all": "concurrently \"cd backend && npm run start:dev\"  \"npm run dev\"",
    "start:prod": "concurrently \"cd backend && npm run start:prod\" \"node server.js\"",
    "build:all": "npm run build && cd backend && npm run build"
  },
  "dependencies": {
    "@capacitor/android": "^5.6.0",
    "@capacitor/core": "^5.6.0",
    "@capacitor/network": "^5.0.6",
    "@nestjs/platform-socket.io": "^11.0.11",
    "@nestjs/platform-ws": "^11.0.11",
    "@nestjs/websockets": "^11.0.11",
    "@quasar/extras": "^1.16.9",
    "@supabase/supabase-js": "^2.52.0",
    "@types/pdfkit": "^0.13.9",
    "axios": "^1.6.7",
    "claude": "^0.1.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dexie": "^3.2.4",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "pdfkit": "^0.16.0",
    "pg": "^8.11.3",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.3",
    "quasar": "^2.14.6",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "vue": "^3.4.38",
    "vue-router": "^4.3.0",
    "ws": "^8.18.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^5.6.0",
    "@quasar/vite-plugin": "^1.6.0",
    "@types/crypto-js": "^4.2.1",
    "@types/express": "^4.17.21",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.11.24",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/tsconfig": "^0.5.1",
    "concurrently": "^9.1.2",
    "sass": "^1.71.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.2",
    "vue-tsc": "^2.1.4"
